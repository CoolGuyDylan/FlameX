-- Scripts:

local function JSOSHP_fake_script() -- PlayerAvatar.PlayerIconScript 
	local script = Instance.new('LocalScript', PlayerAvatar)

	local players = game:GetService("Players")
	local player = players.LocalPlayer
	
	local imageLabel = script.Parent
	
	local success, result = pcall(function()
		return players:GetUserThumbnailAsync(player.UserId, Enum.ThumbnailType.AvatarThumbnail, Enum.ThumbnailSize.Size420x420)
	end)
	
	if success then
		if result then
			imageLabel.Image = result
		end
	else
		print("why fail did - retor (the flamex guy)")
	end
end
coroutine.wrap(JSOSHP_fake_script)()
local function URGE_fake_script() -- PlayButton_2.ExecuteScript 
	local script = Instance.new('LocalScript', PlayButton_2)

	function onClicked()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
	end
	
	script.Parent.MouseButton1Click:Connect(onClicked)
end
coroutine.wrap(URGE_fake_script)()
local function QPIYT_fake_script() -- HomeButton.PageChangerScript 
	local script = Instance.new('LocalScript', HomeButton)

	local mainFrame = script.Parent.Parent.Parent.MainFrame
	
	function onClicked()
		mainFrame.HomeFrame.Visible = true
		mainFrame.SettingsFrame.Visible = false
		mainFrame.LocalPlayerFrame.Visible = false
		mainFrame.OtherScriptsFrame.Visible = false
		mainFrame.ScriptsFrame.Visible = false
	end
	
	script.Parent.MouseButton1Click:Connect(onClicked)
end
coroutine.wrap(QPIYT_fake_script)()
